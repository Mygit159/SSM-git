<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!--设置日志-->
    <settings>
        <setting name="logImpl" value="STDOUT_LOGGING"/>
    </settings>
    <!--设置别名-->
    <typeAliases>
        <package name="com.HuLi.domain"/>
    </typeAliases>
    <!--指定mapper文件的位置-->
    <mappers>
        <!--<mapper resource="com\HuLi\dao\StudentDao.xml"/>-->
       <package name="com.HuLi.dao"/>
    </mappers>
</configuration>